      <div class="basic_info basic_item_m">
        {{#unless (isEmpty video_url)}}<h3 class="tit">상품 인증 동영상 및 상사정보</h3>
        {{else}}<h3 class="tit">상품 소유 상사 정보</h3>
        {{/unless}}

        <div>
          {{#if (isEmpty video_url)}}
            <img src="{{../static}}cars/{{img_url}}" alt="" width="100%" />
          {{else}}
            <div class="video_section">
              <iframe class="iframe_video" width="100%" src="https://www.youtube.com/embed/{{video_url}}?autoplay=0&rel=0&showinfo=1&controls=1&autohide=1&theme=light&disablekb=1&modestbranding=1" frameborder="0" allowfullscreen></iframe>
            </div>
          {{/if}}

          <div class="dealer_info dealer_info_m">
            <table class="tb_store_info table">
              <tr>
                <th>시도 / 지역</th>
                <td>{{province}} / {{region}}</td>
              </tr>
              <tr>
                <th>상사명</th>
                <td>{{store_name}}</td>
              </tr>
              <tr>
                <th>대표명</th>
                <td>{{ceo_name}}</td>
              </tr>
              {{#if (isEquals level 'S')}}
                <tr>
                  <th>대표 연락처</th>
                  <td><a href="tel:{{ceo_tel}}">{{ceo_tel}}</a></td>
                </tr>
              {{else}}
                <tr>
                  <th>고객센터</th>
                  <td><a href="tel:16008218">1600-8218</a></td>
                </tr>
              {{/if}}
            </table>
            {{#if (isEquals level 'S')}}
              <span class="special_store_badge_m"><img src="/static/images/sub/badge_special_store.jpg" alt="" width="100" /></span>
            {{else}}
              <span class="general_store_badge_m"><img src="/static/images/sub/badge_general_store.jpg" alt="" width="80" /></span>
            {{/if}}

            {{#if (isEquals level 'S')}}
              <div class="home_link_area_m">
                <a href="/store/{{eng_name}}" target="_blank">
                  <img src="/static/images/sub/btn_special_store_home.jpg" alt="특별제휴상사 홈페이지" width="100%" />
                </a>
              </div>
            {{/if}}
          </div>

        </div>
      </div>

        <div class="basic_info basic_item_m">
          <h3 class="tit">상품 기본 정보</h3>
          <table class="table table_info">
            <tr>
              <th class="bold align-left">형식 및 모델연도</th>
              <td class="align-right">{{year}}년형</td>
            </tr>
            <tr>
              <th class="bold align-left">제조사</th>
              <td class="align-right">{{company}}</td>
            </tr>
            <tr>
              <th class="bold align-left">모델명</th>
              <td class="align-right">{{model}}</td>
            </tr>
            <tr>
              <th class="bold align-left">연료</th>
              <td class="align-right">
                {{#if (isEquals fuel 'G') }}가솔린{{else if (isEquals fuel 'D')}}디젤{{else if (isEquals fuel 'H')}}하이브리드{{else if (isEquals fuel 'E')}}전기{{else if (isEquals fuel 'L')}}LPG{{else}}?{{/if}}
              </td>
            </tr>
            {{! todo 변속기 }}
            {{#if color}}
              <tr>
                <th class="bold align-left">색상</th>
                <td class="align-right">{{color}}</td>
              </tr>
            {{/if}}
            <tr>
              <th class="bold align-left">주행거리</th>
              <td class="align-right">{{comma-number kilo}} km</td>
            </tr>
            {{#if car_number}}
              <tr>
                <th class="bold align-left">차량번호</th>
                <td class="align-right">{{car_number}}</td>
              </tr>
            {{/if}}
            {{#if item_id}}
              <tr>
                <th class="bold align-left">차대번호</th>
                <td class="align-right">{{item_id}}</td>
              </tr>
            {{/if}}
            {{#if power}}
              <tr>
                <th class="bold align-left">마력(RPM)</th>
                <td class="align-right">{{power}}({{rpm}})</td>
              </tr>
            {{/if}}
            {{#if fuel_rate}}
              <tr>
                <th class="bold align-left">연비</th>
                <td class="align-right">{{fuel_rate}}</td>
              </tr>
            {{/if}}
            <tr>
              <th class="bold align-left">사고/교환</th>
              <td class="align-right">하단 성능기록부참고</td>
            </tr>
          </table>
        </div>

