{{#if (isEquals isMobile 1 )}}
  <div class="m_container">
    {{>hd}}
    <div class="sub_detail">
      {{#each info}}
        <div class="imgs_area swiper-container">
          {{#unless (isEmpty video_url)}}<img src="/static/images/icon/ico_medal.png" alt="차대리인증" width="40" class="ico_medal" />{{/unless}}
          <div class="swiper-wrapper">
            {{#if (isEmpty ../images)}}
              <div class="swiper-slide">
                <img src="{{../static}}cars/{{img_url}}" alt="" class="img_thumbnail" />
              </div>
            {{else}}
              {{#each ../images}}
                <div class="swiper-slide">
                  <img src="{{../../static}}item_images/{{../img_url}}/{{this}}" alt="" class="img_thumbnail" />
                </div>
              {{/each}}
            {{/if}}
          </div>

          <div class="swiper-pagination"></div>
          {{#if (isEquals ../isMobile 0)}}
            <div class="swiper-button-prev"></div>
            <div class="swiper-button-next"></div>
          {{/if}}
        </div>

        <div class="item_info" style="border-bottom: 1px solid #b1b1b1;">
          <p class="tit"><strong>{{model}}</strong></p>
          <p class="info">
            {{{year}}}년형
            <span class="split">|</span>
            {{#if (isEquals fuel 'G') }}가솔린{{else if (isEquals fuel 'D')}}디젤{{else if (isEquals fuel 'H')}}하이브리드{{else if (isEquals fuel 'E')}}전기{{else if (isEquals fuel 'L')}}LPG{{else}}?{{/if}}
            <span class="split">|</span>
            {{comma-number kilo}}km
          </p>

          <p class="badge_area clearfix">
            {{#unless (isEmpty video_url)}}<span class="badge">차대리인증</span>&nbsp;{{/unless}}
            <span class="badge">할인가능</span>&nbsp;
            {{! todo 조건에 따라서 뱃지 이름을 변경하거나 없앤다. }}
            <!--<span class="badge">신차급</span>-->
            {{#if (isEquals sold 1)}}<span class="badge ico_sold">거래완료</span>{{/if}}
            <a href="javascript:alert('신고하기 기능은 준비중입니다.');" class="btn_alert_item_m">
              <img src="/static/images/sub/btn_alert_item.jpg" alt="신고하기" width="80" />
            </a>
          </p>

          <table class="table tb_price_info_m">
            <tr>
              <td class="price_section">
                <span class="txt">상품 시작가</span><br />
                <strong class="price">{{price}}</strong>
                <span class="unit">만원</span>
              </td>
              <td>
                <span class="txt">총예상비용 (대략)</span><br />
                <strong class="price">~ {{amountPrice price type_id fuel}}</strong>
                <span class="unit">만원이하</span>
              </td>
            </tr>
          </table>
        </div>

        <div class="center req_lowprice_wrp_m">
          <a href="/request/2dealer/{{id}}" class="btn_low_price_m" target="_blank">
            "차주"에게 <strong>"최저가"</strong>를 물어봐!!
            <i class="glyphicon glyphicon-menu-right"></i>
          </a>
        </div>

        <div class="btn_req_area_m {{#if (isEmpty blog_url)}}btn-2-req{{else}}btn-3-req{{/if}} clearfix">
          {{#if (isEmpty blog_url)}}
            <a href="https://talk.naver.com/ct/wc4ktq" class="btn_req_m btn-talk-req-m" target="_blank">
              <i class="ico_talktalk_m"><img src="/static/images/sub/mobile/bl_talktalk.gif" alt="" width="20" /></i>
              톡톡
            </a>
            <a href="http://plus.kakao.com/home/ov0qv67l" class="btn_req_m btn-plus-req-m" target="_blank">
              <i class="ico_talktalk_m"><img src="/static/images/sub/mobile/bl_pluschingu.gif" alt="" width="23" /></i>
              플친
            </a>
          {{else}}
            <a href="http://blog.naver.com/chadaeli/{{blog_url}}" class="btn_req_m btn-blog-req-m" target="_blank">
              <i class="ico_talktalk_m"><img src="/static/images/sub/mobile/bl_blog.gif" alt="" width="20" /></i>
              블로그
            </a>
            <a href="https://talk.naver.com/ct/wc4ktq" class="btn_req_m btn-talk-req-m" target="_blank">
              <i class="ico_talktalk_m"><img src="/static/images/sub/mobile/bl_talktalk.gif" alt="" width="20" /></i>
              톡톡
            </a>
            <a href="http://plus.kakao.com/home/ov0qv67l" class="btn_req_m btn-plus-req-m" target="_blank">
              <i class="ico_talktalk_m"><img src="/static/images/sub/mobile/bl_pluschingu.gif" alt="" width="23" /></i>
              플친
            </a>
          {{/if}}

        </div>

        <div style="padding: 0 15px;">
          <a href="tel:16008218" class="btn-customer-req-m">
            <i class="ico_talktalk_m"><img src="/static/images/sub/mobile/bl_tel.gif" alt="" width="23" /></i>
            고객센터 1600-8218
          </a>
        </div>



      <!--{{#unless (isEmpty blog_url)}}-->
        <!--<div class="info-btn-area">-->
          <!--<a href="http://blog.naver.com/chadaeli/{{blog_url}}" class="btn-blog-info" target="_blank">-->
            <!--네이버 블로그에서 보기-->
          <!--</a>-->
        <!--</div>-->
      <!--{{/unless}}-->

      <!--{{#if video_url}}-->
        <!--<div class="video_section">-->
          <!--<iframe class="iframe_video" width="100%" src="https://www.youtube.com/embed/{{video_url}}?autoplay=0&rel=0&showinfo=1&controls=1&autohide=1&theme=light&disablekb=1&modestbranding=1" frameborder="0" allowfullscreen></iframe>-->
        <!--</div>-->
